{
  "name": "celinenguyen.com",
  "version": "1.0.0",
  "description": "Portfolio-ish website for Celine Nguyen",
  "main": "index.js",
  "//": "See https://github.com/npm/npm/issues/8918#issuecomment-328316465 re: licensing",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true source/style/ -o docs/style",
    "build-task:typefaces": "cp -r source/style/type docs/style/type",
    "build-task:favicon": "cp source/favicon.ico docs/favicon.ico",
    "build-task:favicon-large": "cp source/favicon-152.png docs/favicon-152.png",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'source/scss/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celinenguyen/celinenguyen.com.git"
  },
  "author": "Celine Nguyen <hello@celinenguyen.com>",
  "license": "UNLICENSED",
  "homepage": "https://celinenguyen.com",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "chokidar-cli": "^1.2.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.1"
  }
}
