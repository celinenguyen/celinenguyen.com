  "description": "Portfolio-ish website for Celine Nguyen",
  "main": "index.js",
  "scripts": {
    "sass:build": "node-sass-chokidar --source-map true source/includes/style/ -o docs/includes/style",
    "sass:watch": "chokidar 'source/includes/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
