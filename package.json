  "//": "See https://github.com/npm/npm/issues/8918#issuecomment-328316465 re: licensing",
  "homepage": "https://celinenguyen.com",
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.3"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "postcss": "^8.1.2",
    "postcss-cli": "^8.1.0"
  }
}
